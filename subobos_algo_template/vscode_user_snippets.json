  "fastpower": {
    "prefix": "fastpow",
    "body": [
      "",
      "template <typename T, typename U>",
      "inline T pow(const T& a, const T& b, const U& mod) {",
      "  assert(b >= 0);",
      "  T x = a, p = b, ans = 1;",
      "  while (p > 0) {",
      "    if (p & 1) ans = ans * x % mod;",
      "    x = x * x % mod;",
      "    p >>= 1;",
      "  }",
      "  return ans % mod;",
      "}",
      ""
    ]
